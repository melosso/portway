# .gitattributes
# This file is used to define attributes for paths in the repository.

# Exclude directory from GitHub language statistics
Pages/* linguist-vendored
Documentation/* linguist-vendored
# And now for the individual file types
*.html linguist-vendored=true
*.htm linguist-vendored=true
*.css linguist-vendored=true
*.conf linguist-vendored=true
*.yml linguist-vendored=true
*.sh linguist-vendored=true
*.sln linguist-vendored=true
*.pack filter=lfs diff=lfs merge=lfs -text
*.idx filter=lfs diff=lfs merge=lfs -text
